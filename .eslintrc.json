{
    "extends": [
        "eslint-config-airbnb-base",
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "rules": {
        "class-methods-use-this": "off",
        "function-paren-newline": "off",
        "indent": ["warn", 4, { "SwitchCase": 1 }],
        "object-curly-newline": "off"
    }
}
